{
  "name": "sanity-plugin-asset-source-unsplash",
  "version": "1.0.1",
  "description": "Use images from Unsplash.com in your Sanity Studio",
  "keywords": [
    "sanity",
    "plugin",
    "images",
    "unsplash",
    "assets",
    "source"
  ],
  "homepage": "https://github.com/sanity-io/sanity-plugin-asset-source-unsplash#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-plugin-asset-source-unsplash/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/sanity-plugin-asset-source-unsplash.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "v2-incompatible.js",
    "sanity.json"
  ],
  "browserslist": [
    "> 0.2% and supports es6-module and supports es6-module-dynamic-import and not dead and not IE 11",
    "maintained node versions"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint .",
    "prebuild": "npm run clean && plugin-kit verify-package --silent && pkg-utils",
    "build": "pkg-utils build --strict",
    "watch": "pkg-utils watch",
    "link-watch": "plugin-kit link-watch",
    "test": "tsc --noEmit",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^1.0.4",
    "react-infinite-scroll-component": "^6.1.0",
    "react-photo-album": "^1.16.1",
    "rxjs": "^7.5.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@sanity/pkg-utils": "^1.20.4",
    "@sanity/plugin-kit": "^2.2.0",
    "@sanity/semantic-release-preset": "^2.0.4",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "sanity": "^3.0.6",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18",
    "sanity": "^3.0.0",
    "styled-components": "^5.2"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "sanityExchangeUrl": "https://www.sanity.io/plugins/sanity-plugin-asset-source-unsplash"
}
