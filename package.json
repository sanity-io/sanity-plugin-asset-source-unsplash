    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "prompt": "^1.3.0",
    "react": "^18",
    "react-dom": "^18",
    "replace-in-file": "^6.3.5",
    "rimraf": "^3.0.2",
    "sanity": "3.0.0-rc.0",
